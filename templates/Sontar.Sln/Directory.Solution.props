<!--
This file is not affected by template update and can be used for
customization at the solution level.
 -->
<Project>
  <!-- Version -->
  <PropertyGroup>
    <VersionPrefix>1.0.0</VersionPrefix>
    <VersionSuffix></VersionSuffix>
  </PropertyGroup>

  <!-- These come from the template symbols-->
  <PropertyGroup>
    <GitHubRepoOwner>sontarru</GitHubRepoOwner>
    <!--#if(GitHubRepoName != '')-->
    <GitHubRepoName>{GitHubRepoName}</GitHubRepoName>
    <!--#else-->
    <GitHubRepoName>Sontar.Sln</GitHubRepoName>
    <!--#endif-->
    <Company>sontar.ru</Company>
  </PropertyGroup>

  <!-- Assembly & Package properties -->
  <PropertyGroup>
    <Product>Sontar.Sln</Product>
    <Description>Sontar.Sln</Description>
    <Authors>$(Company)</Authors>
    <Copyright>Â© $(Company), 2025</Copyright>
    <RepositoryType>git</RepositoryType>
    <RepositoryName>$(GitHubRepoName)</RepositoryName>
    <RepositoryUrl>https://github.com/$(GitHubRepoOwner)/$(GitHubRepoName)</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/$(GitHubRepoOwner)/$(GitHubRepoName)</PackageProjectUrl>
    <PackageTags><!-- insert tags here --></PackageTags>
  </PropertyGroup>
</Project>
